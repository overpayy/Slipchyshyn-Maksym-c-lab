#include <iostream>
using namespace std;
float perOfSquare(float);
int main()
{
    float len, area;
    cout << "Enter the Length of Square: ";
    cin >> len;
    area = len * len;
    cout << "\nArea = " << area;
    cout << "\nPerimeter = " << perOfSquare(len);
    cout << endl;
    return 0;   
}
float areaOfSquare(float len)
{
    return (len * len);
}
float perOfSquare(float len)
{
    return (4 * len);
}
